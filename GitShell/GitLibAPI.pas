  GIT_CHECKOUT_NONE = 0;
  GIT_CHECKOUT_SAFE = 1 shl 0;
  GIT_CHECKOUT_FORCE = 1 shl 1;
  GIT_CHECKOUT_RECREATE_MISSING = 1 shl 2;
  GIT_CHECKOUT_ALLOW_CONFLICTS = 1 shl 4;
